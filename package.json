    "source-map-support": "^0.5.19",
    "sqlite3": "^5.0.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "codecov": "^3.7.2",
    "electron": "9.2.0",
    "electron-builder": "^22.8.0",
    "electron-webpack": "^2.8.2",
    "eslint": "^7.6.0",
    "eslint-config-google": "^0.14.0",
    "nock": "^13.0.3",
    "nyc": "^15.1.0",
    "tmp": "^0.2.1",
    "webpack": "~4.44.1"
  }
